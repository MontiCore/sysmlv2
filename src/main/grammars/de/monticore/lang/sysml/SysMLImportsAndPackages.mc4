package de.monticore.lang.sysml;


component grammar SysMLImportsAndPackages extends de.monticore.lang.sysml.SysMLBasis {

  SysMLImportStatement extends MCImportStatement = SysMLModifier
    ("import" | ["alias"]) MCQualifiedName (("::" | ".") Star:["*"])? ("as"  Name)? ";" ;

  scope SysMLPackage implements SysMLElement = "package" Name? "{"
    (SysMLElement | SysMLImportStatement)*
  "}";
}
