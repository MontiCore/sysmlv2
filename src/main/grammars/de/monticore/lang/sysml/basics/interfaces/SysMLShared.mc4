package de.monticore.lang.sysml.basics.interfaces;

component grammar SysMLShared extends de.monticore.lang.sysml.basics.interfaces.SysMLNamesBasis{
  interface Unit; //A Unit is a starting production

  interface NestedDefinitionMember;
  interface NestedUsageMember;

  /* DEFINITION MEMBERSHIPS */
  interface NonPortStructureUsageMember; //Referencing other elements in a production
  interface BehaviorUsageMember; //Referencing other elements in a production

  /* ASSOCIATION BLOCK MEMBERSHIPS */
  interface AssociationUsageMember;

  interface FeatureTyping;

  interface Parameter;

  token FromToDigitsDotDot = ('0'..'9')+ "..";
  /*Remark: The following does not exist in Xtext, but since many NTs use the following structure we introduced this NT.
  */

  interface TypePart;
  SysMLNameAndTypePart implements SysMLType = SysMLName TypePart?;
  astrule SysMLNameAndTypePart =
    method public String getName(){
      return this.getSysMLName().getName();
    }
  ;
}
