package de.monticore.lang.sysmlvariant.basics.objectoriented;

component grammar SysMLNamesOO extends de.monticore.lang.sysml.basics.interfaces.SysMLNamesBasis  {
  DotQualifiedNameOO implements QualifiedName = (SysMLName || ".")+; //Only dots allowed here

  SysMLNameOO implements SysMLName = name:Name; //No unrestricted name (No 'my name')

  //Remark in Xtext this is Name
  astrule SysMLNameOO =
    method public String getNameForPrettyPrinting(){
      return this.getName();
    }
  ;
}
