package de.monticore.lang;

component grammar SysMLInterfaces
    extends de.monticore.lang.SysMLBasis {

  symbol scope InterfaceDef implements SysMLType =
    "interface" "def" Name DefSpecialization* ("(" (SysMLParameter || ",")* ")")?
    ("{"
      ("end" src:MCQualifiedName DefSpecialization* ";"| SysMLElement)*
    "}" | ";") ;//TODO end definieren f√ºr interface

  symbol scope InterfaceUsage implements SysMLElement =
    "interface" (Name? UsageSpecialization* "connect" )? source:MCQualifiedName "to" target:MCQualifiedName ";" ;


}
