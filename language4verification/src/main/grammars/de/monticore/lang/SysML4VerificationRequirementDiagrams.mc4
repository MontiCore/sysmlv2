package de.monticore.lang;

grammar SysML4VerificationRequirementDiagrams extends SysMLRequirementDiagrams,
                                                      SysML4VerificationBlockDiagrams {
    @Override
    RequirementDef implements ParametricsElement =
        "requirement" "def" ("<" humanId:String ">")? Name ("(" (SysMLParameter || ",")* ")")?
        SysMLSpecialization?
        SysMLRefinement?
        RequirementBody;
}
